{
  "expo": {
    "name": "todo",
    "slug": "rn-todo-app",
    "privacy": "public",
    "sdkVersion": "42.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "me.listup.todo-app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "me.listup.todo_app",
      "versionCode": 1,
      "permissions": ["ACCESS_COARSE_LOCATION", "WRITE_EXTERNAL_STORAGE"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
